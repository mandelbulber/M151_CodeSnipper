@using CodeSnipper_Web.ViewModels;
@model List<CodeSnipper_Web.ViewModels.SnippetViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <div class="row">
        @foreach (SnippetViewModel snippet in Model)
        {
            <div class="col-sm-6 col-md-4 col-lg-3">
                <div>
                    <partial name="/Views/Shared/Snippet.cshtml" model="snippet" />
                </div>
            </div>
        }
    </div>
</div>